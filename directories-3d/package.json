{
  "name": "directories-3d",
  "version": "0.0.1",
  "private": true,
  "description": "Reference app to showcase relay's 3D(Data-driven dependencies) feature",
  "scripts": {
    "dev": "concurrently \"next dev\" \"relay-compiler --watch\"",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "relay": "mkdir -p __generated__ && relay-compiler",
    "analyze": "cross-env ANALYZE=true next build",
    "prettier": "prettier --write '{,!(node_modules|__generated__|\\.next)/**}*.{mjs,js,json,ts,tsx}'"
  },
  "license": "MIT",
  "dependencies": {
    "@apollo/server": "^4.7.3",
    "@as-integrations/next": "^2.0.0",
    "@graphql-tools/schema": "^10.0.0",
    "@headlessui/react": "^1.7.2",
    "@heroicons/react": "^2.0.11",
    "@testing-library/jest-dom": "^5.16.5",
    "chance": "^1.1.8",
    "graphql": "^16.6.0",
    "graphql-relay": "^0.10.0",
    "graphql-tag": "^2.12.6",
    "jest-environment-jsdom": "^29.3.1",
    "micro": "^9.4.1",
    "next": "latest",
    "prettier": "^2.7.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-relay": "main",
    "relay-runtime": "main"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^13.4.10",
    "@testing-library/react": "^13.4.0",
    "@types/chance": "^1.1.3",
    "@types/jest": "^29.2.5",
    "@types/node": "18.7.21",
    "@types/react": "18.0.21",
    "@types/react-dom": "18.0.6",
    "@types/react-relay": "^14.1.1",
    "@types/relay-runtime": "^14.1.3",
    "autoprefixer": "^10.4.12",
    "concurrently": "^7.4.0",
    "cross-env": "^7.0.3",
    "eslint": "8.24.0",
    "eslint-config-next": "12.3.1",
    "jest": "^29.3.1",
    "postcss": "^8.4.16",
    "relay-compiler": "main",
    "tailwindcss": "^3.1.8",
    "typescript": "4.8.3"
  },
  "prettier": {
    "bracketSameLine": true,
    "bracketSpacing": false,
    "singleQuote": true,
    "trailingComma": "all"
  }
}
